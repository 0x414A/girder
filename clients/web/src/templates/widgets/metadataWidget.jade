include metadatumViewMixin
if (accessLevel >= girder.AccessType.WRITE)
  button.btn.btn-sm.btn-primary.g-item-metadata-add-button(title="Add Metadata")
    i.icon-plus
.g-item-metadata-header
  i.icon-tags
  |  Metadata
.g-item-metadata-container
  each keyValue in meta
    .g-item-metadata-row(g-key="#{keyValue.key}", g-value="#{keyValue.value}")
      +metadatum(keyValue.key, keyValue.value, accessLevel, girder)
