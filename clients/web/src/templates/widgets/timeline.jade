.g-timeline-widget-wrapper
  .g-timeline-container(style="background-color: #{defaultColor};")
    each segment in segments
      .g-timeline-segment(
        title=segment.tooltip, class=segment.class,
        style="left: #{segment.left}; width: #{segment.width};")
    each point in points
      .g-timeline-point(
        title=point.tooltip, class=point.class, style="left: #{point.left}")
